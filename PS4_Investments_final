   ],
   "source": [
    "#Annualize returns\n",
    "dt = 12\n",
    "\n",
    "df['mcap'] = df['shrout']*np.abs(df['prc'])\n",
    "df['wlag'] = df.groupby('permno')['mcap'].shift(1).to_frame()\n",
    "df['decile']=1+df[['beta','date']].groupby('date').transform(lambda x: pd.qcut(x, 10, labels=False))\n",
    "\n",
    "vwexret = df.groupby(['date','decile']).apply(lambda x:((x['ret']*dt-x['tmytm'])*\n",
    "                                                         x['wlag']).sum()/x['wlag'].sum()).to_frame().fillna(0)\n",
    "avg_vwexret = vwexret.groupby('decile').mean().rename(columns={0:'avg_vwexret'})\n",
    "beta_vwexret=df.groupby(['decile']).apply(lambda x:(x['beta']*x['wlag']).sum()/\n",
    "                                                    x['wlag'].sum()).to_frame().rename(columns={0:'beta_vwexret'})\n",
    "\n",
    "x=np.linspace(0.4,2.1,10)\n",
    "capm = df['tmytm'].mean()*dt + (df['vwretd'].mean()-df['tmytm'].mean())*x*dt\n",
    "fitted = sm.OLS(avg_vwexret,sm.add_constant(beta_vwexret)).fit().params\n",
    "\n",
    "plt.plot(x,capm,color='black',label='SML')\n",
    "plt.plot(beta_vwexret,fitted[0]+fitted[1]*beta_vwexret,color='red',label='Fitted')\n",
    "plt.scatter(beta_vwexret,avg_vwexret,label='Beta portfolios')\n",
    "plt.title('Value weighted portfolios')\n",
    "plt.grid()\n",
    "plt.legend()\n",
    "plt.show()\n"
   ]
  },
