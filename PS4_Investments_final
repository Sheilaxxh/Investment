    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import statsmodels.api as sm\n",
    "\n",
    "import wrds\n",
    "db = wrds.Connection(wrds_username='maheo')\n",
    "#db.create_pgpass_file()\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "7f5de2bc",
   "metadata": {},
   "outputs": [],
   "source": [
    "market = db.raw_sql(\"select date, vwretd from crsp.msi where date>='1928-01-01' and date<='2004-12-31'\", \n",
    "                    date_cols=['date'])\n",
    "stocks = db.raw_sql(\"select a.permno, a.date, b.shrcd, b.exchcd, a.ret, a.shrout, a.prc from crsp.msf \"\n",
    "                    \"as a left join crsp.msenames as b on a.permno=b.permno and b.namedt<=a.date and \"\n",
    "                    \"a.date<=b.nameendt where a.date between '01/01/1928' and '12/31/2004' and b.exchcd \"\n",
    "                    \"between 1 and 2 and b.shrcd between 10 and 11\", date_cols=['date'])\n",
    "tbill = db.raw_sql(\"select mcaldt, tmytm from crsp.tfz_mth_rf where kytreasnox=2000001 and \"\n",
    "                   \"mcaldt>='1928-01-01' and mcaldt<='2004-12-31'\", date_cols=['mcaldt'])\n",
    "#continuously compounded to monthly returns\n",
    "tbill['tmytm']=np.exp(tbill['tmytm']/12/100)-1\n",
    "tbill = tbill.rename(columns={'mcaldt':'date'})\n",
    "\n",
    "df = pd.merge(stocks,tbill,on='date')\n",
    "df = pd.merge(df,market,on='date')\n"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "8b3fcdb3-a91e-4439-9f4e-4a5bfc520460",
   "metadata": {},
   "source": [
    "## a)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "3f759e2f",
   "metadata": {},
   "outputs": [],
   "source": [
    "olsreg = df.groupby('permno').apply(lambda x : sm.OLS(x['ret'] - x['tmytm'],\n",
    "                                                      sm.add_constant(x['vwretd'] - x['tmytm'])).fit().params)\n",
    "coeffs = olsreg.unstack(level=-1)\n",
    "coeffs.columns = ['beta','alpha']\n",
    "df = pd.merge(df, coeffs['beta'], how='left', left_on=['permno'], right_on='permno')\n"
   ]
  },
