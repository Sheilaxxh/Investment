    "ewexret = df.groupby(['date','decile']).apply(lambda x:(x['ret']*dt-x['tmytm']).sum()/len(x)).to_frame().fillna(0)\n",
    "avg_ewexret = ewexret.groupby('decile').mean().rename(columns={0:'avg_ewexret'})\n",
    "beta_ewexret=df.groupby(['decile']).apply(lambda x:(x['beta'].sum()/\n",
    "                                                    len(x))).to_frame().rename(columns={0:'beta_ewexret'})\n",
    "\n",
    "fitted = sm.OLS(avg_ewexret,sm.add_constant(beta_ewexret)).fit().params\n",
    "\n",
    "plt.plot(x,capm,color='black',label='SML')\n",
    "plt.plot(beta_ewexret,fitted[0]+fitted[1]*beta_ewexret,color='red',label='Fitted')\n",
    "plt.scatter(beta_ewexret,avg_ewexret,label='Beta portfolios')\n",
    "plt.title('Equal weighted portfolios')\n",
    "plt.grid()\n",
    "plt.legend()\n",
    "plt.show()\n"
